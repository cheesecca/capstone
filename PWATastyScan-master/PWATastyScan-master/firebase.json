{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "source": "homepage",
        "destination": "/homepage.html"
      },
      {
        "source": "search",
        "destination": "/search.html"
      },
      {
        "source": "list",
        "destination": "/list.html"
      }
    ]
  }
}
